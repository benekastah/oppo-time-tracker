
h1 Time Tracker

- var punching_in = true;
- if (timestamps)
  - each timestamp in timestamps
    - punching_in = timestamp.time_in && timestamp.time_out
!= partial("punch_in_out", {punching_in: punching_in})

ul.key
  li.bill-pending Not billed
  li.billed Billed
  li.payment-pending Payment pending
  li.payment-received Payment received

hr

- if (billing_periods)
  - each period in billing_periods
    != partial("billing_period", {period: period})

!= partial("punch_in_out", {punching_in: punching_in})